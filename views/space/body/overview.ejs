        <div class="my-4">
            <form action="" class="form w-100">
                <fieldset class="fieldset">
                    <legend class="legend">SANDBOX</legend>
                    <div class="row p-0 m-0">
                        <div class="col-md-4 p-3">
                            <div class="py-2">
                                <strong>
                                    <h6 class="text-info text-sm"><i class="fas fa-circle text-success">&nbsp;</i>
                                        <%= project.title %>
                                    </h6>
                                </strong>
                                <input type="text" value="<%= project._id %>" name="projId" id="projId" hidden>
                                    <label class="switch">
                                        <input type="checkbox" id="myCheck" name="myCheck">
                                        <span class="slider round"></span>
                                    </label>
                                
                            </div>
                        </div>
                        <div class="col-md-8 bg-bright p-2">
                            <div class="row p-0 m-0">
                                <div class="col-md-6">
                                    <div class="card">
                                        <div class="p-2">
                                            <small class="heading">
                                                <strong class="text-muted">Categories:</strong>
                                                <strong data-name="kfield">
                                                    <%= project.category.length %></strong>
                                            </small>
                                            <canvas id="myChart"></canvas>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="card">
                                        <div class="p-2">
                                            <small class="heading">
                                                <strong class="text-muted">Questions:</strong>
                                                <% let _count = project.category 
                                                    let ls = []
                                                    for(let i=0; i < _count.length; i++) {
                                                         let _countq = _count[i].question
                                                            for(j=0; j < _countq.length; j++) {
                                                                ls.push(_countq)
                                                                %>

                                                <% }} %>
                                                <strong data-name="kfield">
                                                    <%= ls.length %>
                                                </strong>
                                            </small>
                                        </div>
                                        <div class="con"></div>
                                        <div class="w-100">
                                            <ul class="nav w-100">
                                                <li class="overview-item head py-1">
                                                    <small class="px-2">Most Read Questions</small>
                                                    <small>clicks</small>
                                                </li>
                                                <div class="w-100 question">
                                                    <% if (project.category != 'undefined') { 
                                                        let categories = project.category %>
                                                    <% for (let i=0; i < categories.length; i++) { 
                                                        let questions = categories[i].question %>
                                                    <% for(let j=0; j < questions.length; j++) { %>
                                                    <% if (questions[j].clicks > 2 ) { %>
                                                    <li class="overview-item py-2 text-info">
                                                        <small class="px-2 o-cont">
                                                            <%= questions[j].question %></small>
                                                        <small class="kfield" data-name="kfield">
                                                            <%= questions[j].clicks %> </small>
                                                    </li>
                                                    <% } %>
                                                    <% } %>
                                                    <% } %>
                                                    <% } %>
                                                </div>
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </fieldset>
            </form>
        </div>
    </div>
