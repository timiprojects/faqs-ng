<small style="font-size: 12px;padding: 0 10px;">Page <i>
        <%= current_page %></i> of <i>
        <%= pageCount %></i></small>
<nav aria-label="Page navigation example">
    <% if (pageCount > 1) { %>
    <ul class="pagination justify-content-end pagination-sm" style="margin:0;padding:0;">
        <% if (current_page == 1) { %>
        <li class="page-item disabled"><a class="page-link">First</a></li>
        <% } else { %>
        <li class="page-item">
            <a class="page-link" href="?page=1" tabindex="-1" aria-disabled="true">Previous</a>
        </li>
        <% } %>
        <% let i = (Number(current_page) > 3 ? Number(current_page) - 2 : 1) %>
        <% if (i !== 1) { %>
        <li class="page-item disabled"><a class="page-link">...</a></li>
        <% } %>
        <% for (i; i <= (Number(current_page) + 2) && i <= pageCount; i++) { %>
        <% if (i == current_page) { %>
        <li class="page-item active">
            <a class="page-link">
                <%= i %>
            </a>
        </li>
        <% } else { %>
        <li class="page-item">
            <a class="page-link" href="?page=<%= i %>" tabindex="-1" aria-disabled="true">
                <%= i %></a>
        </li>
        <% } %>
        <% if (i == Number(current_page) + 2 && i < pageCount) { %>
        <li class="page-item disabled"><a class="page-link">...</a></li>
        <% } %>
        <% } %>
        <% if (current_page == pageCount) { %>
        <li class="page-item disabled"><a class="page-link">Last</a></li>
        <% } else { %>
        <li class="page-item">
            <a class="page-link" href="?page=<%= pageCount %>" tabindex="-1" aria-disabled="true">Last</a>
        </li>
        <% } %>
    </ul>
    <% } %>
</nav>


