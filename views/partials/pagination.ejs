<nav aria-label="Page navigation example">
    <% if (pageCount >= 1) { %>
    <ul class="pagination justify-content-end pagination-sm" style="margin:0;padding:0;">
        <% if (current_page == 1) { %>
        <li class="page-item disabled">
            <a class="page-link" tabindex="-1" aria-disabled="true">Previous</a>
        </li>
        <% } else { %>
        <li class="page-item">
            <a class="page-link" href="?page=<%= --current_page %>" tabindex="-1" aria-disabled="true">Previous</a>
        </li>
        <% } %>
        <% let i = 1 %>
        <% if(i !== 1) { %> 
            <li class="page-item disabled"><a href="#">...</a></li>
        <% } %>
        <% for(i; i <= pageCount; i++) { %> 
            <% if (Number(current_page) == i) { %> 
                <li class="page-item">
                    <a class="page-link" href="?page=<%= i %>"><%= i %></a>
                </li>
            <% } else { %>
                <li class="page-item ">
                    <a class="page-link" href="?page=<%= i %>"><%= i %></a>
                </li>
            <% } %>
        <% } %>
        <% if(current_page == pageCount) { %>
        <li class="page-item disabled" aria-disabled="true" disabled>
            <a class="page-link" >Last</a>
        </li>
        <% } else { %>
        <li class="page-item ">
            <a class="page-link"  href="?page=<%= pageCount %>">Last</a>
        </li>
        <% } %>
    </ul>
    <% } %>
</nav>

